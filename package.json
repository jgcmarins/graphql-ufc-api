{
  "name": "graphql-ufc-api",
  "description": "GraphQL Server on top of UFC REST API",
  "version": "2.0.0",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "dataloader": "^1.4.0",
    "dotenv-safe": "^6.1.0",
    "graphql": "^14.2.1",
    "graphql-playground-middleware": "^1.1.2",
    "graphql-playground-middleware-koa": "^1.6.4",
    "graphql-relay": "^0.6.0",
    "isomorphic-fetch": "^2.2.1",
    "kcors": "^2.2.2",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-compose": "^4.1.0",
    "koa-convert": "^1.2.0",
    "koa-cors": "^0.0.16",
    "koa-graphql": "^0.8.0",
    "koa-graphql-batch": "^1.1.0",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "pretty-error": "^2.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.3.2",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.4.4",
    "@babel/plugin-transform-flow-strip-types": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/graphql-relay": "^0.4.6",
    "@types/kcors": "^2.2.3",
    "@types/koa-logger": "^3.1.0",
    "@types/koa-router": "^7.0.31",
    "auto-reload-webpack-plugin": "^1.1.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-typescript": "^1.0.2",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^2.1.0",
    "jest": "^24.7.1",
    "jest-fetch-mock": "^2.1.2",
    "jest-junit": "^6.3.0",
    "jest-runner-eslint": "^0.7.3",
    "lint-staged": "8.1.5",
    "prettier": "^1.14.2",
    "repl": "^0.1.3",
    "repl-promised": "^0.1.0",
    "repl.history": "^0.1.4",
    "rimraf": "^2.6.2",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "typescript-eslint-parser": "^22.0.0",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "license": "MIT",
  "private": true,
  "lint-staged": {
    "*.js": [
      "yarn prettier",
      "eslint --fix",
      "git add"
    ],
    "*.ts": [
      "yarn prettier",
      "tslint --fix",
      "git add"
    ],
    "*.yml": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "build": "yarn clear && babel src --extensions \".es6,.js,.es,.jsx,.mjs,.ts\" --ignore *.spec.js --out-dir dist --copy-files",
    "build:webpack": "yarn clear && webpack --env.mode=prod",
    "clear": "rimraf ./dist",
    "jest:lint": "jest --config jest-eslint.config.js",
    "lint": "eslint src/**",
    "precommit": "lint-staged",
    "prettier": "prettier --write --single-quote true --trailing-comma all --print-width 120",
    "repl": "nodemon --config ./repl/nodemon.json ./repl.js --exec babel-node",
    "serve": "node dist/index.js",
    "start": "yarn clear && webpack --env.mode=dev",
    "test": "jest",
    "test:watch": "jest --watch --coverage",
    "typecheck": "tsc",
    "update-schema": "babel-node --extensions \".es6,.js,.es,.jsx,.mjs,.ts\" ./scripts/updateSchema.js"
  }
}
